{
    "name": "mdnayeemsarker/role-permission",
    "description": "A Laravel package for role-based permissions with section permissions.",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mdnayeemsarker\\RolePermission\\": "src/"
        }
    },
    "autoload-dev": {
